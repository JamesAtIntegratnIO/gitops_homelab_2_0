apiVersion: platform.integratn.tech/v1alpha1
kind: VCluster
metadata:
  name: dev-team-cluster
  namespace: vcluster-dev-team
spec:
  name: dev-team
  targetNamespace: vcluster-dev-team
  preset: dev
  k8sVersion: "1.34"
  isolationMode: standard
  resources:
    requests:
      cpu: "200m"
      memory: "512Mi"
    limits:
      cpu: "1000m"
      memory: "1Gi"
  persistence:
    enabled: true
    size: "5Gi"
    storageClass: config-nfs-client
  networking:
    clusterDomain: cluster.local
  hostname: dev-team.integratn.tech
  subnet: 10.0.6.0/24
  apiPort: 8443
